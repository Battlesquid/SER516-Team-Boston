<?xml version="1.0" encoding="UTF-8"?>

<?import atlantafx.base.controls.ModalPane?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<fx:root prefHeight="720.0" prefWidth="1280.0" type="StackPane" xmlns="http://javafx.com/javafx/21.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ModalPane fx:id="modalPane" />
      <HBox>
         <children>
            <VBox alignment="CENTER" maxWidth="-Infinity" spacing="10.0" styleClass="sidebar" HBox.hgrow="SOMETIMES">
               <children>
                  <Button fx:id="home" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" />
                  <Region VBox.vgrow="ALWAYS" />
                  <Button fx:id="settings" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" />
                  <Button fx:id="account" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
            <VBox fx:id="root" spacing="10.0" HBox.hgrow="ALWAYS">
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
            </VBox>
         </children>
      </HBox>
   </children>
</fx:root>
